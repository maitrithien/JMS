@model DMS.Models.JobModels

@using (Html.BeginForm(null, null, FormMethod.Post, new { id = "frm-feed-dialog" }))
{
    <div class="content-wrapper">
        @Html.Partial("GridFeeds")
    </div>
    <div class="content-wrapper bottom-toolbar">
        <button id="btnCancel" type="button">Đóng</button>
        <script type="text/javascript">
            $("#btnCancel").kendoButton({
                width: 120,
                icon: "close",
                click: function () {
                    var win = $("#feedDialog").data("kendoWindow");
                    if (win) {
                        win.close();
                    }
                }
            });
        </script>
    </div>
        
}






