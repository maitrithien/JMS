<div class="content-wrapper">
     @Html.Partial("GridFeeds")
</div>
<div class="content-wrapper bottom-toolbar">
    <button id="btnFeedCancel" type="button">Đóng</button>
    <script type="text/javascript">
        $("#btnFeedCancel").kendoButton({
            width: 120,
            icon: "close",
            click: function () {
                var win = $("#feedDialog").data("kendoWindow");
                if (win) {
                    win.close();
                }
            }
        });
    </script>
</div>




